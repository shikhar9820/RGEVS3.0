<%- include ('../partials/header'); -%>
<h1 class="text-primary text-center">Name of the poll</h1>
  <div style="width: 60%; margin: 50px auto;">
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">House</th>
        <th scope="col">Post</th>
        <th scope="col">Name</th>
        <th scope="col">Image</th>


      </tr>
    </thead>
    <tbody>
     
      <% candidate.forEach(function(candidate){ %>
         <tr>
          <th scope="row">1</th>
          
          <td><%= candidate.house %></td>
          <td><%= candidate.post %></td>
          <td><%= candidate.name %></td>
          <td><img src="../<%= candidate.image %>" width="100" height="150"></td>

          <% console.log(candidate.image);%>
          <% console.log(candidate.image);%>

  
        </tr>
         <% }); %>
    </tbody>
  </table>
  <div style="width: 30%; margin: 50px auto;">
    <a href="/test" class="btn btn-lg btn-primary btn-block" role="button" aria-pressed="true">Finish</a> 
  </div>

  </div>
</div>

<%- include ('../partials/footer'); -%>
